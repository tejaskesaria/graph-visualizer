    "@vue/compiler-sfc": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "jest": "^29.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue3-jest": "^29.0.0",
    "babel-jest": "^29.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/core": "^7.0.0",
    "jsdom": "^21.1.0"
